@charset "utf-8";

/* CSS Document */

@font-face {
    font-family: 'Calibri';
    src: url('fonts/Calibri-Bold.eot');
    src: url('fonts/Calibri-Bold.eot?#iefix') format('embedded-opentype'), url('fonts/Calibri-Bold.woff') format('woff'), url('fonts/Calibri-Bold.ttf') format('truetype');
    font-weight: bold;
    font-style: normal;
}
@import url('https://fonts.googleapis.com/css?family=Pathway+Gothic+One');
body {
    font-family: 'Pathway Gothic One', sans-serif;
    margin: 0;
    padding: 0;
    overflow-x: hidden;
}
h1,
h2,
h3,
h4,
h5,
h6,
p {
    margin: 0;
    padding: 0;
    line-height: normal;
}
input,
select,
textarea,
option {
    outline: none;
}
ul,
li {
    list-style-type: none;
}
a {
    color: #a98244;
    text-decoration: none !important;
    outline: 0 !important;
}
a:hover {
    color: #95733c;
    text-decoration: none !important;
    outline: 0 !important;
}
a img {
    border: none;
    outline: 0;
}
.fright {
    float: right;
}
.wrapper {
    background-color: #fff;
}
.page-header {
    border-bottom: none;
    margin: 0 auto;
    padding-bottom: 0;
}
.skip-link .label {
    border-radius: 0;
    display: inline;
    font-size: 100%;
    font-weight: normal;
    line-height: auto;
    padding: 0;
}
.skip-account .icon {
    color: #fff;
}
.skip-cart .icon {
    color: #fff;
}
.skip-cart:hover:not(.skip-active) .icon {
    background-position: -51px -95px;
}
.header-language-background {
    display: none;
}
.topMenu {
    position: absolute;
    top: 10px;
    right: 0;
    z-index: 99;
}
.topMenu a.aboutus {
    font-size: 15px;
    font-family: 'Pathway Gothic One', sans-serif;
}
.logo {
    padding: 0;
    background: url(../images/logo_back.jpg) left top repeat-y;
    position: relative;
}
.logo:before {
    position: absolute;
    content: "";
    display: block;
    background: #bababa url(../images/logo_left.jpg) left top repeat-x;
    height: 100%;
    width: 5000px;
    right: 100%;
    top: 0;
}
.logo:after {
    position: absolute;
    content: "";
    display: block;
    background: #bababa url(../images/logo_right.jpg) left top repeat-x;
    height: 100%;
    width: 5000px;
    left: 100%;
    top: 0;
}
.logo:hover {
    opacity: 1.0 !important;
}
.topMenu a.aboutus .label {
  border-radius: 0;
  color: #000;
  font-size: 100%;
  font-weight: 400;
}
.header-language-background .header-language-container {
    text-align: center;
}
.header-language-background .welcome-msg {
    float: none;
}
.logo .large {
  /*height: 118px !important;*/
  width: 309px !important;
}
.logo {
    max-width: 309px;
    min-height: auto;
    padding: 30px 0;
}
