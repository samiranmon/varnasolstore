/* responsive design */

@media (min-width:992px) and (max-width:1199px) {
    .banner-text h2 {
        font-size: 30px;
    }
    .banner-text-mid {
        transform: translateY(20%)
    }
    .theme-product {
        margin: 0 auto;
        max-width: 80%;
    }
    .list-section-mid li {
        width: 22%;
    }
    .content-text-left {
        width: 85%;
    }
    .content-text-left h2 {
        font-size: 20px;
    }
    .content-text-left p {
        font-size: 13px;
    }
    .content-text-right {
        width: 85%;
    }
    .content-text-right h2 {
        font-size: 20px;
    }
    .about-varnasol-right .testimonial_sidebar_text {
        font-size: 13px;
    }
}

@media (min-width:769px) and (max-width:991px) {
    .nav-primary a {
        font-size: 13px;
    }
    .nav-primary a.level0 {
        padding: 0 6px;
    }
    .list-section-mid li {
        width: 22%;
    }
    .list-text-content h2 {
        font-size: 11px;
    }
    .list-text-content {
        padding: 10px 15px;
    }
    .t-1 img {
        padding: 7px 0;
    }
    .t-3 img {
        padding: 7px 0;
    }
    .t-4 img {
        padding: 7px 0;
    }
    .one-part img {
        width: 75%;
    }
    .img-box img {
        width: 100%;
    }
    .about-varnasol-left {
        background: #ebebeb;
        overflow: hidden;
    }
    .about-varnasol-right {
        background: #fff;
        overflow: hidden;
    }
    .banner-text h2 {
        font-size: 20px;
    }
    .banner-text-mid {
        transform: translateY(20%)
    }
    .theme-product {
        margin: 0 auto;
        max-width: 70%;
    }
    .testimonials_title {
        font-size: 30px;
    }
}

@media (min-width:581px) and (max-width:768px) {
    .list-section-mid li {
        display: inline-block;
        float: none;
        margin: 0 10px 20px !important;
        width: 46%;
    }
    .banner-text h2 {
        font-size: 20px;
    }
    .banner-text p {
        font-size: 12px;
    }
    .banner-text a {
        font-size: 11px;
        padding: 5px 12px;
        margin-top: 0;
    }
    .t-1 img {
        margin-left: 0px;
    }
    .t-1 a {
        left: 0px;
    }
}

@media (max-width:768px) {
    .theme-product {
        margin: 0 auto;
        max-width: 70%;
    }
    .about-varnasol-div {
        display: block;
    }
    .about-varnasol-left {
        background: #ebebeb;
        overflow: hidden;
        width: 100%;
    }
    .about-varnasol-right {
        background: #fff;
        overflow: hidden;
        width: 100%;
    }
    .content-text-left {
        width: 100%;
        margin: 0;
        padding: 30px 15px;
    }
    .content-text-right {
        width: 100%;
        margin: 0;
        padding: 30px 15px;
    }
    .one-part,
    .two-part,
    .three-part,
    .four-part {
        width: 50%
    }
    .custom-skLk {
        position: absolute;
        text-align: right;
        top: 75px;
        width: 100%;
        z-index: 9999;
    }
    .skip-links {
        clear: none;
        display: inline-block;
        overflow: hidden;
        vertical-align: middle;
        width: auto;
    }
    .toll-free {
        display: inline-block;
        position: relative;
        right: 0;
        top: 0;
        vertical-align: middle;
        margin-right: 10px;
    }
    .skip-content.skip-active {
        clear: both;
    }
    .custom-skLk .skip-link .label {
        display: none;
    }
    .custom-skLk .skip-nav,
    .custom-skLk .skip-search {
        width: auto;
        padding: 0 10px;
    }
    .img-box img {
        width: 100%;
    }
    .footer-address {
        text-align: center;
    }
    .one-part img {
        display: inline;
        margin-top: 20px;
        margin-bottom: 20px;
    }
    .testimonial_l,
    .testimonial_r {
        float: left;
        padding: 30px 15px;
        width: 100%;
    }
    .testimonial_l {
        border-right: 0;
    }
    .testimonials_title {
        font-size: 28px;
    }
}

@media (max-width:580px) {
    .one-part,
    .two-part,
    .three-part,
    .four-part {
        width: 100%
    }
    .footer .links {
        margin-bottom: 0;
    }
    .list-section-mid li {
        margin: 0 0 15px !important;
        width: 100%;
    }
    .custom-skLk {
        display: table;
        margin: 0 auto;
        position: relative;
        text-align: right;
        top: 0px;
        width: auto;
        z-index: 9999;
    }
    .logo {
        display: table;
        float: none;
        margin: 0 auto;
        min-height: 44px;
        min-width: 100px;
        padding: 10px;
        text-align: center;
        width: auto;
    }
    .page-header-container {
        margin-bottom: 20px;
    }
    .banner-text {
        display: none;
    }
    .slideshow-prev,
    .slideshow-next {
        display: none;
    }
    .list-text-content {
        padding: 50px;
    }
    .theme-product h2 {
        font-size: 28px;
    }
    .content-text-left h2 {
        font-size: 30px;
    }
    .content-text-right h2 {
        font-size: 30px;
    }
    .one-part {
        text-align: center;
    }
    .list-section {
        padding: 0 0 20px;
    }
    .cms-page h2 {
        font-size: 26px;
    }
    .g-i,
    .right-t {
        width: 100%;
    }
    .g-i {
        margin-bottom: 30px;
    }
    .col-4,
    .col-3,
    .col-5 {
        width: 100%;
    }
    .capt {
        top: -20px
    }
}

@media (max-width:360px) {
    .custom-skLk .skip-nav,
    .custom-skLk .skip-search {
        padding: 0 5px;
    }
    .custom-skLk {
        width: 100%;
    }
    .list-text-content {
        padding: 25px 15px;
    }
    .theme-product .owl-prev {
        left: -35px;
    }
    .theme-product .owl-next {
        right: -35px;
    }
}