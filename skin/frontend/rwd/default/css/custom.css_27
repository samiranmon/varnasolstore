.manufacturer-list
{
    clear: both;
}
.manufacturer-list > ul
{
    margin-right: 28px;
}
.manufacturer-list ul li
{
    list-style: none;
    border-bottom: 1px solid #c1c1c1;
    padding: 15px 0;
}

.manufacturer_image
{
    margin: 0 auto;
}

