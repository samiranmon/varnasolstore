@import 'https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800';
.toll-free {
    font-family: "Open Sans", sans-serif;
    font-weight: 600;
    right: 240px;
    position: absolute;
    top: 38px;
}

.toll-free p {
    color: #010101;
    font-size: 18px;
}

.toll-free span {
    color: #4d4d4d;
    font-weight: 300;
}

.banner-text {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
}

.banner-text-mid {
    margin: 0 auto;
    max-width: 1115px;
    padding: 0 15px;
    transform: translateY(40%);
    position: relative;
    top: 40%;
}

.banner-text h2 {
    font-size: 42px;
    color: #fff;
    font-weight: 400;
    max-width: 500px;
    margin: 0;
}

.banner-text h2 span {
    font-weight: 700;
}

.banner-text p {
    font-size: 14px;
    color: #fff;
    font-weight: 300;
    max-width: 500px;
    padding: 10px 0;
    font-family: "Raleway", "Helvetica Neue", Verdana, Arial, sans-serif;
}

.banner-text a {
    border: 1px solid #d0902a;
    border-radius: 20px;
    clear: both;
    color: #d0902a;
    display: inline;
    float: left;
    font-size: 14px;
    margin-top: 15px;
    padding: 10px 29px;
    text-transform: uppercase;
}

.banner-text a:hover {
    text-decoration: none;
    background: #d0902a;
    color: #fff;
}

.cms-index-index .main-container {
    padding: 0;
    width: 100%;
    max-width: 100%;
}

.about-varnasol-div {
    padding: 0;
    margin: 0;
    list-style: none;
    display: -webkit-box;
    display: -moz-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-flow: row wrap;
    justify-content: space-around;
}

.about-varnasol-left {
    background: url(../images/av-left.jpg);
    padding: 5px;
    width: 50%;
    background-size: 100% 100%;
}

.about-varnasol-right {
    background: url(../images/av-right.jpg);
    padding: 5px;
    background-size: 100% 100%;
    width: 50%;
}

@media only screen and (max-width: 1366px) {
    .about-varnasol-left {
        background: url(../images/1366px-left.png);
        padding: 5px;
        width: 50%;
        background-size: 100% 100%;
    }
    .about-varnasol-right {
        background: url(../images/1366px-right.png);
        padding: 5px;
        background-size: 100% 100%;
        width: 50%;
    }
    .our-feature {
        background: url(../images/1366px-feature-products.png);
    }
}

.content-text-left {
    width: 70%;
    float: right;
    text-align: left;
    padding-top: 30px;
    margin-right: 50px;
    padding-bottom: 15px;
}

.content-text-left h2 {
    color: #000000;
    font-size: 34px;
    text-transform: capitalize;
    border-bottom: 1px solid #ddd;
    padding-bottom: 10px;
}

.content-text-left p {
    font-family: "Open Sans", sans-serif;
    color: #454545;
    font-size: 15px;
    line-height: normal;
    padding: 0px 0 10px;
    margin: 0;
    font-weight: 300;
}

.content-text-left a {
    border: 1px solid #d0902a;
    border-radius: 20px;
    clear: both;
    color: #d0902a;
    display: inline;
    float: left;
    font-size: 14px;
    margin-top: 15px;
    padding: 10px 29px;
}

.content-text-left a:hover {
    text-decoration: none;
    background: #d0902a;
    color: #fff;
}

.content-text-right {
    width: 70%;
    float: left;
    text-align: left;
    padding-top: 30px;
    margin-left: 50px;
}

.content-text-right h2 {
    color: #000000;
    font-size: 34px;
    text-transform: capitalize;
    border-bottom: 1px solid #ddd;
    padding-bottom: 10px;
    position: relative;
}

.content-text-right h2 a {
    color: #cf8313;
    font-size: 14px;
    position: absolute;
    right: 0;
    bottom: 15px;
}

.content-text-right h2 a span img {
    display: inline;
}

.content-text-right h2 a:hover {
    text-decoration: none;
}

.about-varnasol-right li {
    list-style: none;
    margin: 0 0 10px!important;
    border-bottom: 1px dotted #9d9d9d;
    padding-bottom: 15px;
}

.about-varnasol-right li:last-child {
    border-bottom: 0;
    padding-bottom: 0;
    margin-bottom: 0!important;
}

.about-varnasol-right .rating-box {
    display: none;
}

.testimonial_sidebar_name > img {
    border-radius: 50%;
    height: 80px;
    width: 80px;
}

.about-varnasol-right .testimonial_sidebar_name {
    float: left;
}

.about-varnasol-right .testimonial_sidebar_text {
    margin-left: 105px;
    font-family: "Open Sans", sans-serif;
    color: #454545;
    font-size: 15px;
    font-weight: 300;
    position: relative;
}

.author-name {
    font-size: 13px;
    color: #454545;
    font-family: "Raleway", "Helvetica Neue", Verdana, Arial, sans-serif;
    font-style: italic;
    display: block;
    width: 95%;
    padding: 0px 0 0;
    text-align: right;
    text-transform: capitalize;
    font-weight: 500;
}

@media only screen and (min-width: 1420px) {
    .logo {
        max-width: 235px;
    }
    .banner-text-mid {
        top: 40%;
    }
    .content-text-left {
        padding-bottom: 80px;
        padding-top: 80px;
        width: 51%;
    }
    .content-text-left p {
        font-size: 18px;
    }
    .content-text-right {
        padding-bottom: 80px;
        padding-top: 80px;
        width: 50%;
    }
    .banner-text p {
        font-size: 18px;
        padding: 30px 0;
    }
    .about-varnasol-right .testimonial_sidebar_text {
        font-size: 18px;
    }
    .author-name {
        font-size: 15px;
    }
}

.our-feature {
    background: url(../images/our-feature.jpg);
    width: 100%;
    padding: 75px 0;
}

.theme-product {
    max-width: 960px;
    margin: 0 auto;
}

.theme-product h2 {
    font-size: 34px;
    color: #fff;
    text-align: center;
    padding-bottom: 40px;
    margin: 0;
}

.our-feature .item {
    background: #fff;
    padding: 10px;
    margin: 0 10px;
    min-height: 300px;
}

.our-feature .img-box {
    border: 1px solid #ddd;
    padding: 10px;
    text-align: center;
    min-height: 300px;
}

.our-feature .img-box img {
    display: inline;
}

.prod-text-box {
    text-align: center;
    font-family: "Raleway", "Helvetica Neue", Verdana, Arial, sans-serif;
    padding-bottom: 20px;
}

.prod-text-box p {
    margin: 0;
    padding: 0 0 5px;
    color: #515151;
    font-size: 16px;
    font-weight: 400;
}

.prod-text-box a {
    margin: 0;
    padding: 0;
    color: #e49e2e;
    font-size: 14px;
}

.prod-text-box a:hover {
    text-decoration: none;
}

.prod-text-box a span {
    font-size: 10px;
    padding-left: 5px;
}

.theme-product .owl-prev {
    float: left;
    position: relative;
    left: -60px;
    background: url(../images/prev-img.png);
    width: 41px;
    height: 79px;
}

.theme-product .owl-next {
    float: right;
    position: relative;
    right: -60px;
    background: url(../images/next-img.png);
    width: 41px;
    height: 79px;
}

.list-section-mid {
    max-width: 1140px;
    padding: 0 15px;
    margin: 0 auto;
    text-align: center;
}

.list-section-mid li {
    float: left;
    position: relative;
    list-style: none;
    width: 23%;
    margin: 0 10px!important;
}

.list-section-mid li:first-child {
    margin-left: 0;
}

.list-section-mid li:last-child {
    margin-right: 0;
}

.list-section-mid li img {
    width: 100%;
}

.list-text-content {
    position: absolute;
    right: 0;
    top: 0;
    padding: 25px 15px;
    text-align: left;
}

.list-text-content img {
    width: auto!important;
}

.list-text-content h2 {
    color: #fff;
    font-size: 18px;
    margin: 0;
    padding: 0;
    font-weight: 500;
}

.list-text-content a {
    color: #eaa332;
    font-size: 12px;
    margin: 0;
    padding: 0 10px 0 0;
    background: url(../images/list-aerow.png) no-repeat 100% 5px;
}

.list-text-content a:hover {
    text-decoration: underline;
}

.list-section {
    padding-bottom: 80px;
    overflow: hidden;
    padding-top: 50px;
}

.t-1 img {
    padding: 15px 0;
    margin-left: 20px;
}

.t-1 a {
    position: relative;
    left: 20px;
}

.t-3 img {
    padding: 15px 0;
}

.t-4 img {
    padding: 15px 0;
}

.footer-all {
    border-top: 4px solid #d0d0d0;
    background: #f0f0f0;
}

.footer-address {
    border-top: 1px solid #d7d7d7;
    position: relative;
    padding-top: 25px;
}

.footer-address:before {
    position: absolute;
    left: 0;
    width: 100%;
    top: 0;
    border-top: 1px solid #fff;
    content: "";
}

.footer-row {
    margin: 0;
}

.one-part {
    float: left;
    padding: 15px 0;
    width: 30%;
}

.two-part {
    float: left;
    padding: 15px 0;
    width: 20%;
}

.three-part {
    float: left;
    padding: 15px 0;
    width: 30%;
}

.four-part {
    float: left;
    padding: 15px 0;
    width: 20%;
}

.footer-row .links {
    width: 100%;
}

.footer-toll-free {
    position: relative;
    left: 0;
    border: 1px solid #dedede;
    display: inline-block;
    padding: 14px;
    top: 20px;
}

.social-media {
    clear: both;
}

.social-media li {
    float: left;
    padding-right: 5px;
}

.cms-page {
    padding: 30px 0;
}

.cms-page h2 {
    text-align: center;
    font-size: 35px;
    font-family: "Open Sans", sans-serif;
    text-transform: inherit;
    margin-bottom: 1.5em!important;
}

.cms-page h2 span {
    color: #e8ab4a;
}

.cms-page p {
    font-size: 15px!important;
}

.cms-page p {
    margin-bottom: 1.5em!important;
}

.cms-page p span {
    font-size: 18px;
    display: block;
    color: #000000;
}

.cms-page h3 {
    font-size: 22px;
    font-family: "Open Sans", sans-serif;
    text-transform: inherit;
    color: #000000;
    margin-bottom: 30px!important;
}

.cms-page h3 span {
    text-transform: inherit;
    font-weight: 600;
    font-size: 23px;
    color: #2a2a2a;
}

ul.adviceMenu {
    list-style: none;
    margin: 0;
    padding: 0;
    margin-bottom: 50px;
}

ul.adviceMenu li {
    display: inline-block;
    font-size: 16px!important;
    margin: 0;
    padding: 0;
    margin-right: 30px;
    font-style: normal!important;
    font-family: "Open Sans", sans-serif!important;
}

.innerpageHeader {
    -webkit-box-shadow: 0px -1px 13px 0px rgba(0, 0, 0, 0.10);
    -moz-box-shadow: 0px -1px 13px 0px rgba(0, 0, 0, 0.10);
    box-shadow: 0px -1px 13px 0px rgba(0, 0, 0, 0.10);
}

.capt {
    text-align: center;
    position: relative;
    top: -45px;
}

.m-row {
    margin-left: -15px;
    margin-right: -15px;
}

.g-i {
    width: 25%;
    float: left;
    padding-left: 15px;
    padding-right: 15px;
}

.right-t {
    width: 75%;
    float: left;
    padding-left: 15px;
    padding-right: 15px;
}

.right-t p {
    margin-bottom: 30px;
}

.col-4,
.col-3,
.col-5 {
    float: left;
    padding-right: 15px;
    padding-left: 15px;
}

.col-4 {
    width: 33.333%;
}

.col-3 {
    width: 25%;
}

.col-5 {
    width: 41.66666667%;
}

.cms-page .col-4 p,
.cms-page .col-3 p,
.cms-page .col-5 p {
    margin-bottom: 5px!important;
}

.s-box {
    margin-bottom: 30px;
}

.page-header:before,
.page-header:after {
    display: table;
    content: "";
}

.page-header:after {
    clear: both;
}

.testimonials_title {
    color: #e49e2e;
    text-align: center;
    font-size: 35px;
    font-family: "Open Sans", sans-serif;
    text-transform: inherit;
    margin-bottom: 1.5em!important;
}

.testimonials_title span {
    color: #2a2a2a;
}

.testimonials-thumb {
    border: 1px solid #c1c1c1;
}

.testimonials-thumb .description-box {
    font-size: 15px;
    font-family: "Open Sans", sans-serif;
    color: #747474;
    font-style: italic;
}

.testimonial_l,
.testimonial_r {
    float: left;
    padding: 45px 15px;
    border-bottom: 1px solid #c1c1c1;
    width: 50%;
    box-sizing: border-box;
    position: relative;
    margin-bottom: -1px;
}

.testimonial_l {
    border-right: 1px solid #c1c1c1;
}

.clearer {
    clear: both;
}

.testimonial_image img {
    float: left;
    margin-right: 15px;
    -webkit-border-radius: 50px;
    -moz-border-radius: 50px;
    border-radius: 50px;
    width: 59px;
    height: 59px;
}

.testimonial_image {
    margin-top: 22px;
    padding-top: 22px;
    padding-left: 5%;
    background: url(../images/c-top-line.png) no-repeat 5% top;
}

.testimonial_image span {
    font-size: 14px;
    font-family: "Open Sans", sans-serif;
    color: #747474;
    font-style: italic;
    padding-top: 17px;
    display: inline-block;
}




